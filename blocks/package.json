		"@wordpress/browserslist-config": "2.2.2",
		"@wordpress/compose": "2.1.0",
		"@wordpress/element": "2.1.4",
		"@wordpress/i18n": "3.0.1",
		"acorn": "6.0.4",
		"babel-eslint": "10.0.1",
		"babel-loader": "8.0.4",
		"cross-env": "5.2.0",
		"css-loader": "1.0.1",
		"eslint": "5.7.0",
		"eslint-config-wordpress": "2.0.0",
		"eslint-plugin-jsx-a11y": "6.1.2",
